{
    "name": "@PLUGIN_NAME@",
    "version": "@PLUGIN_VERSION@", 
    "description": "@PLUGIN_DESCRIPTION@",
    "type": "inference_backend",
    "api_version": "1.0.0",
    "author": "@PLUGIN_AUTHOR@",
    "capabilities": [
        "inference",
        "@PLUGIN_CAPABILITIES@"
    ],
    "dependencies": {
        "modyn_core": ">= 1.0.0"
    },
    "supported_formats": [
        "@PLUGIN_FORMATS@"
    ],
    "configuration": {
        "threads": {
            "type": "integer",
            "default": 4,
            "description": "推理线程数"
        },
        "device": {
            "type": "string", 
            "default": "cpu",
            "description": "推理设备类型"
        },
        "batch_size": {
            "type": "integer",
            "default": 1,
            "description": "批处理大小"
        }
    },
    "entry_point": "@PLUGIN_ENTRY_POINT@",
    "library": "@PLUGIN_LIBRARY@"
} 